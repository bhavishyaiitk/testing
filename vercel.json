{
  "version": 2,
  "builds": [
    {
      "src": "api/Index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/Index.js"
    },
    {
      "src": "/(.*)",
      "dest": "Public/index.html"
    }
  ]
}
